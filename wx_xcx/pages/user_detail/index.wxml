<!--index.wxml-->
<view class="common_container">

  <view class="header">

    <image class="mine_header" src="{{src}}"></image>
  </view>


  <view class="items">

    <view class="child_item" bindtap="goAddPrescriprion">
      <image class="img_style" src="{{xgcf_src}}"></image>
      <view class="text_style">


        修改处方
      </view>
    </view>
    <view class="child_item" bindtap="goTrainReport">
      <image class="img_style" src="{{xlbg_src}}"></image>
      <view class="text_style">
        训练报告
      </view>
    </view>
    <view class="child_item" bindtap="goCheckReport">
      <image class="img_style" src="{{jcbg_src}}" ></image>
      <view class="text_style">
        检查报告
      </view>
    </view>
    <view class="child_item" bindtap="goAlterRecord">
      <image class="img_style" src="{{cfjl_src}}"></image>
      <view class="text_style">
        处方记录
      </view>
    </view>
    <view class="child_item" bindtap="goAssignTrainTimes">
      <image class="img_style" src="{{ckgl_src}}"></image>
      <view class="text_style">
        次卡管理
      </view>
    </view>
  </view>

<button class="scan_code_btn"   catchtap="scanHandle">扫码(剩余{{userInfo.remain_times}}次)</button>
</view>