.train_data .head{
  width: 100%;
  border-bottom: 1px solid #eee;
  box-sizing: border-box;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.head .item-title{
  height:80rpx;
  color:#333;
  font-size:28rpx;
  line-height:80rpx;
  background-color:#fff;
  padding-left:30rpx;
  box-sizing:border-box;

}
.item-title text{
  padding-left: 10rpx;
  position: relative;
}
.item-title text::before{
    content: '';
    display: block;
    width: 6rpx;
    height: 30rpx;
    background-color: #4eb679;
    border-radius: 2px;
    position: absolute;
    left: 0;
    top: 50%;
    transform: translateY(-50%);
}
