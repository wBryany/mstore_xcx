.date_Contain{
    width: 100%;
    height: 80rpx;
    /* position: fixed;
    top: 80rpx;
    z-index: 999; */
    background-color: #f5f5f5;
}
.date_bar{
    display: flex;
    height: 100%;
    position: relative;
}
.date_bar .float_bar{
    width: 160rpx;
    height: 3px;
    background-color: #4BB77F;
    position: absolute;
    bottom: 0;
    left: 45rpx;
    transition: transform 0.5s;
}
.date_bar .date_box{
    width: 250rpx;
    text-align: center;
    font-size: 32rpx;
    color: #999999;
    height: 100%;
    transition: all 0.5s;
}
.date_bar .date_box .date_mes{
    width: 100%;
    height: 42rpx;
    transform: translateY(-19rpx);
    -webkit-transform: translateY(-19rpx);
    /*border-right: 1px solid #EEEEEE;*/
    box-sizing: border-box;
    transition: all 0.5s;
}

.date_bar .date_box .active_m{
    transform: translateY(-4rpx);
    -webkit-transform: translateY(-4rpx);
}
.date_bar .date_box .date_moment{
    width: 100%;
    height: 38rpx;
    box-sizing: border-box;
    transform: translateY(21rpx);
    -webkit-transform: translateY(21rpx);
    opacity: 0;
    transition: all 0.5s;
    font-size: 28rpx;
}
.date_bar .date_box .active_t{
    transform: translateY(0rpx);
    -webkit-transform: translateY(0rpx);
    opacity: 1;
}
.date_bar .active_s{

}
