@import "templet/result.wxss";
page{
    width: 100%;
    height: 100%;
    background-color: #EEEEEE;
}
::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}
.header{
    width: 100%;
    height: 80rpx;
    background-color: #4eb679;
    text-align: center;
    line-height: 80rpx;
    font-size: 30rpx;
    color: #ffffff;
    /* position: fixed;
    top: 0;
    z-index: 999; */
}
/*以下用户检查数据*/
.data_Contain{
    width: 100%;
}
.data_Contain .empty{
    width: 100%;
    height: 400rpx;
    margin-top: 300rpx;
    display: flex;
    justify-content: center;
}
.data_Contain .empty image{
    width: 400rpx;
    height: 400rpx;
}
.scan_container{
    width: 260rpx;
    height: 100rpx;
    /*background-color: #4eb679;*/
    border-radius: 40rpx;
    position: fixed;
    bottom:30rpx;
    left:50%;
    z-index: 999;
    transform:translateX(-50%);
    display: flex;
    align-items: center;
    justify-content: space-around;
    box-sizing: border-box;
    /*padding: 0rpx 23rpx;*/
    /*box-shadow: 0rpx 0rpx 20rpx 10rpx rgba(0,0,0,0.3);*/
}
.scan_container .scan-icon{
    width: 100%;
    height: 100%;
}
.scan_container .scan-text{
    font-size: 30rpx;
    color: #ffffff;
}